# Database Configuration
DATABASE_URL=your_database_url_here

# AI Services Configuration
AI_SERVICES_ENABLED=true
OPENAI_API_KEY=your_openai_api_key_here

# Admin Configuration
ADMIN_SECRET=your_admin_secret_here

# Webhook Security
WEBHOOK_SECRET_TRADING=your_trading_webhook_secret_here
WEBHOOK_SECRET_MARKET=your_market_webhook_secret_here
WEBHOOK_SECRET_GENERIC=your_generic_webhook_secret_here

# Server Configuration
NODE_ENV=development
PORT=5000

# Deployment Configuration (for production)
# Set AI_SERVICES_ENABLED=false during deployment if needed to reduce startup time
# The services will still be available via lazy initialization when API endpoints are called